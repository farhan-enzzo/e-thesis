                <option value="1">Three</option>
                <option value="2">Four</option>
                <option value="3">Five</option>
              </select>
              <button type="button" class="btn btn-outline-primary">Confirm</button>
            </div>

            <div >
              <input class="form-control form-control-sm" type="text" placeholder="Student Id (1)" aria-label=".form-control-sm example" name="Student Id (1)" required>
            </div>
            
            <div>
              <input class="form-control form-control-sm" type="text" placeholder="Student Id (2)" aria-label=".form-control-sm example" name="Student Id (2)" required>
  
            </div>
  
            <div >
              <input class="form-control form-control-sm" type="text" placeholder="Student Id (3)" aria-label=".form-control-sm example" name="Student Id (3)" required>
            
            </div>

            <div >
              <input class="form-control form-control-sm" type="text" placeholder="Student Id (4)" aria-label=".form-control-sm example" name="Student Id (4)">
            
            </div>

            <div >
