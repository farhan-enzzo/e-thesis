              </select>
            </div>

            <div class="input-group input-group-sm mb-3">
              <select class="custom-select form-control" id="">
                <option selected>Available Slots in ?</option>
                <option value="Spring">Spring</option>
                <option value="summer">summer</option>
                <option value="Fall">Fall</option>
              </select>
              <button type="button" class="btn btn-outline-primary">Confirm</button>
            </div>


            <div class="input-group input-group-sm mb-3">
              <input type="search" class="form-control form-control" placeholder="Search Available Supervisor" aria-label="Search" aria-describedby="search-addon" />
              <button type="button" class="btn btn-outline-primary">search</button>
            </div>
          
              <div class="form-group">
                <label for="exampleFormControlTextarea1">Request Note</label>
                <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
              </div>
      
            <div >
              <button class="btn btn-outline-success btn-sm" style =width:550px>Submit</button>
  
            </div>
  
